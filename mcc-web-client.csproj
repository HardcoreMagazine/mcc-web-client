    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>mcc_web_client</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Migration.txt" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="6.0.7" />
